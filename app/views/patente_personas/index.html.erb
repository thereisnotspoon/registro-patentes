<p id="notice"><%= notice %></p>

<h1>Listing Patente Personas</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @patente_personas.each do |patente_persona| %>
      <tr>
        <td><%= link_to 'Show', patente_persona %></td>
        <td><%= link_to 'Edit', edit_patente_persona_path(patente_persona) %></td>
        <td><%= link_to 'Destroy', patente_persona, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Patente persona', new_patente_persona_path %>
